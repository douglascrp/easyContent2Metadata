<?xml version='1.0' encoding='UTF-8'?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
          http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id="com.mimacom.alfresco.utils.DataListsResolver" class="com.mimacom.alfresco.utils.DataListsResolver" >
        <property name="nodeService" ref="NodeService" />
        <property name="searchService" ref="SearchService" />
    </bean>

    <bean id="com.mimacom.alfresco.fillmetadata.FillMetadataBehaviour" class="com.mimacom.alfresco.fillMetadata.FillMetadataBehaviour" >
        <property name="nodeService" ref="NodeService" />
        <property name="contentService" ref="ContentService" />
    </bean>
</beans>
